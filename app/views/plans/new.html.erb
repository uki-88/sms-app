<div class='plan-form'>
  <h1>新規Plan</h1>
  <%=form_with model: @plan, local: true do |f|%>
    <div class='plan-form__field'>
      <div class='plan-form__field--left'>
        <%= f.label :予定タイトル, class: 'plan-form__label'%>
      </div>
      <div class='plan-form__field--right'>
        <%= f.text_field :title, class: 'plan_name plan-form__input', placeholder: 'Plan titleを入力してください'%>
      </div>
    </div>
    <div class='plan-form__field'>
    </div>
    <div class='plan-form__field'>
      <div class='plan-form__field--left'>
        <label class='plan-form__label' for='plam_プランメンバー'>プランメンバー</label>
      </div>
      <div class='plan-form__field--right'>
      </div>
    </div>
    <div class='plan-form__field'>
      <div class='plan-form__field--left'></div>
      <div class='plan-form__field--right'>
        <%= f.submit class: 'plan-form__action-btn'%>
      </div>
    </div>
  <% end %>
</div>
